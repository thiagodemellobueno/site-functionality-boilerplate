{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "site-functionality/subtitle",
  "version": "1.0.0",
  "title": "Subtitle",
  "category": "text",
  "icon": "editor-aligncenter",
  "description": "Add subtitle to content.",
  "attributes": {
    "subtitle": {
      "type": "string"
    },
    "brief_number": {
      "type": "string"
