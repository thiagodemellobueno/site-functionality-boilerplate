  "supports": {
    "html": false,
    "multiple": false,
    "customClassName": true
  },
  "textdomain": "site-functionality",
  "editorScript": "site-functionality",
  "render": "file:./render.php",
  "viewScript": "site-functionality",
  "editorStyle": "file:./index.css"
}